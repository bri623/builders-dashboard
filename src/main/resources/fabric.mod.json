{
	"schemaVersion": 1,
	"id": "builders_dashboard",
	"version": "${version}",
	"name": "Builders Dashboard",
	"description": "HUD notes + photo board for builders. Pin photos/notes on screen, drag/resize polaroids, and import block palettes. Default keys: O = toggle HUD, P = open dashboard (editable in config).",
	"authors": [
		"bri623"
	],
	"contact": {
		"homepage": "https://github.com/bri623/builders-dashboard",
		"sources": "https://github.com/bri623/builders-dashboard"
	},
	"license": "CC0-1.0",
	"icon": "assets/builders_dashboard/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"me.eggzman.builders_dashboard.Builders_dashboard"
		],
		"client": [
			"me.eggzman.builders_dashboard.Builders_dashboardClient"
		],
		"modmenu": [
			"me.eggzman.builders_dashboard.compat.ModMenuIntegration"
		]
	},
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"modmenu": "*"
	}
}
